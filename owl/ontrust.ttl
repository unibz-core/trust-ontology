:Agreement rdfs:subClassOf [
  rdf:type owl:Restriction;
  owl:onProperty gufo:mediates;
  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger;
  owl:onClass :Trustor
], [
  rdf:type owl:Restriction;
  owl:onProperty gufo:mediates;
  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger;
  owl:onClass :Trustee
].
:ThreatEvent rdfs:subClassOf [
  rdf:type owl:Restriction;
  owl:onProperty [ owl:inverseOf gufo:contributedToTrigger ];
  owl:someValuesFrom :ThreateningSituation
].
:ThreateningSituation rdfs:subClassOf [
  rdf:type owl:Restriction;
  owl:onProperty gufo:contributedToTrigger;
  owl:someValuesFrom :ThreatEvent
].
:LossEvent rdfs:subClassOf [
  rdf:type owl:Restriction;
  owl:onProperty [ owl:inverseOf gufo:historicallyDependsOn ];
  owl:someValuesFrom :ThreatEvent
].
:ThreatEvent rdfs:subClassOf [
  rdf:type owl:Restriction;
  owl:onProperty gufo:historicallyDependsOn;
  owl:someValuesFrom :LossEvent
].
:LossSituation rdfs:subClassOf [
  rdf:type owl:Restriction;
  owl:onProperty [ owl:inverseOf gufo:broughtAbout ];
  owl:someValuesFrom :LossEvent
].
:LossEvent rdfs:subClassOf [
  rdf:type owl:Restriction;
  owl:onProperty gufo:broughtAbout;
  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger;
  owl:onClass :LossSituation
].
