##################################################################    General axioms#################################################################

[ rdf:type owl:AllDisjointClasses ] owl:members (rot:Agent rot:Object).
gufo:Object owl:equivalentClass [
  rdf:type owl:Class;
  owl:unionOf (rot:Agent rot:Object)
].


[ rdf:type owl:AllDisjointClasses ] owl:members (rot:AtomicIntention rot:ComplexIntention).
rot:Intention owl:equivalentClass [
  rdf:type owl:Class;
  owl:unionOf (rot:AtomicIntention rot:ComplexIntention)
].


[ rdf:type owl:AllDisjointClasses ] owl:members (rot:DispositionalEvidenceInfluence rot:MentalMomentInfluence rot:TrustInfluence).
[ rdf:type owl:AllDisjointClasses ] owl:members (rot:Agent rot:Object).
gufo:Object owl:equivalentClass [
  rdf:type owl:Class;
  owl:unionOf (rot:Agent rot:Object)
].


[ rdf:type owl:AllDisjointClasses ] owl:members (rot:AtomicIntention rot:ComplexIntention).
rot:Intention owl:equivalentClass [
  rdf:type owl:Class;
  owl:unionOf (rot:AtomicIntention rot:ComplexIntention)
].


[ rdf:type owl:AllDisjointClasses ] owl:members (rot:DispositionalEvidenceInfluence rot:MentalMomentInfluence rot:TrustInfluence).

[ rdf:type owl:AllDisjointClasses ] owl:members (rot:Trust rot:AspectualBelief).

[ rdf:type owl:AllDisjointClasses ] owl:members (rot:TrustDegree rot:BeliefIntensity rot:PerformanceLevel rot:ManifestationLikelihood).